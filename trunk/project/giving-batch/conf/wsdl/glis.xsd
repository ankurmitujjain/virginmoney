<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:glis="http://www.virginmoney.com/type/glis/2009/01"
        targetNamespace="http://www.virginmoney.com/type/glis/2009/01"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:header="http://www.virginmoney.com/type/glis/header/2008/09"
        xmlns:fault="http://www.virginmoney.com/type/glis/faults/2008/01"
        elementFormDefault="qualified">

    <xsd:import namespace="http://www.virginmoney.com/type/glis/header/2008/09" schemaLocation="header.xsd"/>
    <xsd:import namespace="http://www.virginmoney.com/type/glis/faults/2008/01" schemaLocation="fault.xsd"/>

    <xsd:simpleType name="ContactType">
        <xsd:annotation>
            <xsd:documentation>
                Defines possible contact types.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
            <xsd:enumeration value="PRIMARY"/>
            <xsd:enumeration value="OTHER"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="EventCategoryType">
        <xsd:annotation>
            <xsd:documentation>
                Defines possible contact types.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
            <xsd:enumeration value="PROCESS"/>
            <xsd:enumeration value="REPROCESS"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="CharityStatusType">
        <xsd:annotation>
            <xsd:documentation>
                Defines possible contact types.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
            <xsd:enumeration value="Pending"/>
            <xsd:enumeration value="Live"/>
            <xsd:enumeration value="Closed"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="BankAccountCategory">
        <xsd:annotation>
            <xsd:documentation>
                Defines possible bank account types.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
            <xsd:enumeration value="DONATION"/>
            <xsd:enumeration value="TRADING"/>
            <xsd:enumeration value="OTHER"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="EventProcessingTypes">
        <xsd:annotation>
            <xsd:documentation>
                Defines possible event processing types.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
            <xsd:enumeration value="REGISTER-CHARITY"/>
            <xsd:enumeration value="MAINTAIN-CHARITY"/>
            <xsd:enumeration value="CHARITY-REGISTRATION-FEE-PAYMENT-INITIATED"/>
            <xsd:enumeration value="CHARITY-REGISTRATION-FEE-PAYMENT-WAIVED"/>
            <xsd:enumeration value="TRANSACTION-CHARGE-PAID"/>
            <xsd:enumeration value="WEB-DONATIONS-PAYMENT-INITIATED"/>
            <xsd:enumeration value="WEB-DONATIONS-PAYMENT-FAILED"/>
            <xsd:enumeration value="GIFT-AID-CLAIMED"/>
            <xsd:enumeration value="TRANSITIONAL-RELIEF-CLAIMED"/>
            <xsd:enumeration value="CLOSE-CHARITY"/>
            <xsd:enumeration value="BAC-PAYMENT-DUE"/>
            <xsd:enumeration value="WEB-DONATIONS-PAYMENT-COLLECTED"/>
            <xsd:enumeration value="TRANSACTION-FEE-PAID"/>
            <xsd:enumeration value="BAC-PAYMENT-COLLECTED"/>
            <xsd:enumeration value="TRANSITIONAL-RELIEF-RECEIVED"/>
            <xsd:enumeration value="GIFT-AID-RECEIVED"/>
            <xsd:enumeration value="CHARITY-REGISTRATION-FEE-PAYMENT-COLLECTED"/>
            <xsd:enumeration value="PAYMENT-TO-CHARITY-PAYMENT-SETTLED"/>
            <xsd:enumeration value="WEB-EVENT-REGISTRATION-FEE-PAYMENT-INITIATED"/>
            <xsd:enumeration value="WEB-EVENT-REGISTRATION-FEE-PAYMENT-FAILED"/>
            <xsd:enumeration value="WEB-EVENT-REGISTRATION-FEE-PAYMENT-COLLECTED"/>
            <xsd:enumeration value="WEB-REGULAR-DONATIONS-PAYMENT-INITIATED"/>
            <xsd:enumeration value="WEB-REGULAR-DONATIONS-PAYMENT-FAILED"/>
            <xsd:enumeration value="WEB-REGULAR-DONATIONS-PAYMENT-COLLECTED"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="PhoneType">
        <xsd:annotation>
            <xsd:documentation>
                Defines possible phone types.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:restriction base="xsd:token">
            <xsd:enumeration value="DAYTIME"/>
            <xsd:enumeration value="EVENING"/>
            <xsd:enumeration value="MOBILE"/>
            <xsd:enumeration value="OTHER"/>
        </xsd:restriction>
    </xsd:simpleType>

    <complexType name="Phone">
        <xsd:annotation>
            <xsd:documentation>
                Defines the phone details attributes
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="phoneType"     type="glis:PhoneType" minOccurs="1" maxOccurs="1"/>
            <element name="areaCode"      type="xsd:token" minOccurs="1" maxOccurs="1"/>
            <element name="phoneNumber"   type="xsd:token" minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="Contact">
        <xsd:annotation>
            <xsd:documentation>
                Defines the contact details attributes
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="title"        type="xsd:token" minOccurs="1" maxOccurs="1"/>
            <element name="firstName"    type="xsd:token" minOccurs="1" maxOccurs="1"/>
            <element name="surname"      type="xsd:token" minOccurs="1" maxOccurs="1"/>
            <element name="designation"  type="xsd:token" minOccurs="0" maxOccurs="1"/>
            <element name="emailAddress" type="xsd:token" minOccurs="0" maxOccurs="1"/>
            <element name="contactType"  type="glis:ContactType" minOccurs="0" maxOccurs="1"/>
            <element name="phones"       type="glis:Phone"       minOccurs="1" maxOccurs="unbounded"></element>
        </sequence>
    </complexType>

    <complexType name="PaymentCollected">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Payment Collected record
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="charityReference"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="amountDetails"              type="glis:PaymentType"     minOccurs="1" maxOccurs="1"/>
            <element name="eventRef"                   type="xsd:int"              minOccurs="0" maxOccurs="1"/>
            <element name="bankAccountUniqueId"        type="xsd:token"            minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="TransactionChargePaid">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Transaction Charge Paid record
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="creditNoteNumber"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="charityReference"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="chargeAmountDetails"        type="glis:PaymentType"     minOccurs="1" maxOccurs="1"/>
            <element name="vatDetails"                 type="glis:TaxType"         minOccurs="1" maxOccurs="1"/>
            <element name="eventRef"                   type="xsd:int"              minOccurs="0" maxOccurs="1"/>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="bankAccountUniqueId"        type="xsd:token"            minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="TransactionFeePaid">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Transaction Fee Paid record
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="creditNoteNumber"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="charityReference"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="feeAmountDetails"           type="glis:PaymentType"     minOccurs="1" maxOccurs="1"/>
            <element name="vatDetails"                 type="glis:TaxType"         minOccurs="1" maxOccurs="1"/>
            <element name="eventRef"                   type="xsd:int"              minOccurs="0" maxOccurs="1"/>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="bankAccountUniqueId"        type="xsd:token"            minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="TransitionalReliefClaimed">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Donation payment details. Extends the generic payment structure
            </xsd:documentation>
        </xsd:annotation>
        <complexContent>
            <extension base="glis:Transaction">
            </extension>
        </complexContent>
    </complexType>

    <complexType name="BACPaymentDue">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Donation Payments Collected record
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="customer"                   type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="amountDetails"              type="glis:PaymentType"     minOccurs="1" maxOccurs="1"/>
            <element name="eventRef"                   type="xsd:int"              minOccurs="0" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="BACPaymentCollected">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Donation Payments Collected record
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="charityReference"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="amountDetails"              type="glis:PaymentType"     minOccurs="1" maxOccurs="1"/>
            <element name="bankAccountUniqueId"        type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="eventRef"                   type="xsd:int"              minOccurs="0" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="DonationPaymentCollected">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Donation Payments Collected record
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="charityReference"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
              <element name="donationAmount"                  type="xsd:double"/>
            <element name="amountDetails"              type="glis:DonationPayment" minOccurs="1" maxOccurs="1"/>
            <element name="eventRef"                   type="xsd:int"              minOccurs="0" maxOccurs="1"/>
            <element name="bankAccountUniqueId"        type="xsd:token"            minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="GiftAidReceived">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Gift Aid received record
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="charityReference"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="amountDetails"              type="glis:PaymentType"     minOccurs="1" maxOccurs="1"/>
            <element name="bankAccountUniqueId"        type="xsd:token"            minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="TransitionalReliefReceived">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Gift Aid received record
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="charityReference"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="amountDetails"              type="glis:PaymentType"     minOccurs="1" maxOccurs="1"/>
            <element name="bankAccountUniqueId"        type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="eventRef"                   type="xsd:int"            minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="Transaction">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Gift Aid Claimed record
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="amount"                     type="xsd:double"           minOccurs="1" maxOccurs="1"/>
            <element name="Date"                       type="xsd:date"             minOccurs="1" maxOccurs="1"/>
            <element name="customer"                   type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="claimPeriodFrom"            type="xsd:date"             minOccurs="1" maxOccurs="1"/>
            <element name="claimPeriodTo"              type="xsd:date"             minOccurs="1" maxOccurs="1"/>
            <element name="transactionType"            type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="transactionStatus"          type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="eventRef"                   type="xsd:int"              minOccurs="0" maxOccurs="1"/>
            <element name="bankAccountUniqueId"        type="xsd:token"            minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="FailedPayment">
        <xsd:annotation>
            <xsd:documentation>
                Defines the failed payment record
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="creditNoteNumber"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="cardType"                   type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="FailedPaymentDetails"       type="glis:PaymentType"     minOccurs="1" maxOccurs="1"/>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="PaymentInitiated">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Payment Initiated record
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="cardType"                   type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="CardPaymentDetails"         type="glis:PaymentType"     minOccurs="1" maxOccurs="1"/>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="eventRef"                   type="xsd:int"              minOccurs="0" maxOccurs="1"/>
            <element name="bankAccountUniqueId"        type="xsd:token"            minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="CheckSystemStatus">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Request attributes for System availability check
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="sourceSystem"      type="xsd:token" minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="SystemStatusResponse">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Response attributes for System availability check
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="available"      type="xsd:boolean"/>
            <element name="comments"       type="xsd:token"/>
        </sequence>
    </complexType>

    <xsd:complexType name="SubscriptionDetails">
        <xsd:annotation>
            <xsd:documentation>
                Components of a Subscription record.
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="subscriptionBankAccount" type="glis:BankAccountType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="subscriptionAmount"      type="xsd:double"       minOccurs="1" maxOccurs="1"/>
            <xsd:element name="Frequency"               type="xsd:int"          minOccurs="1" maxOccurs="1"/>
            <xsd:element name="PaymentMethod"           type="xsd:token"        minOccurs="1" maxOccurs="1"/>
            <xsd:element name="CollectionDate"          type="xsd:date"         minOccurs="1" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <complexType name="Address">
        <xsd:annotation>
            <xsd:documentation>
                Defines the address attributes
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="addressLine1"       type="xsd:token" minOccurs="1" maxOccurs="1"/>
            <element name="addressLine2"       type="xsd:token" minOccurs="0" maxOccurs="1"/>
            <element name="city"               type="xsd:token" minOccurs="1" maxOccurs="1"/>
            <element name="county_or_state"    type="xsd:token" minOccurs="0" maxOccurs="1"/>
            <element name="postcode_or_zip"    type="xsd:token" minOccurs="1" maxOccurs="1"/>
            <element name="country"            type="xsd:token" minOccurs="0" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="BankAccountType">
        <xsd:annotation>
            <xsd:documentation>
                Defines the bank account attributes
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="sortCode"         type="xsd:token"                minOccurs="1" maxOccurs="1"/>
            <element name="accountNumber"    type="xsd:token"                minOccurs="1" maxOccurs="1"/>
            <element name="accountName"      type="xsd:token"                minOccurs="1" maxOccurs="1"/>
            <element name="bankName"         type="xsd:token"                minOccurs="1" maxOccurs="1"/>
            <element name="branchName"       type="xsd:token"                minOccurs="1" maxOccurs="1"/>
            <element name="accountUniqueId"  type="xsd:token"                minOccurs="1" maxOccurs="1"/>
            <element name="accountType"      type="glis:BankAccountCategory" minOccurs="1" maxOccurs="1"/>
            <element name="primaryUseFlag"   type="xsd:token"                minOccurs="1" maxOccurs="1"/>
            <element name="closeThisAccount" type="xsd:boolean"              minOccurs="0" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="PaymentType">
        <xsd:annotation>
            <xsd:documentation>
                Defines the payment details
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="amount"              type="xsd:double"   minOccurs="1" maxOccurs="1"/>
            <element name="transactionType"     type="xsd:token"    minOccurs="1" maxOccurs="1"/>
            <element name="transactionStatus"   type="xsd:token"    minOccurs="1" maxOccurs="1"/>
            <element name="Date"                type="xsd:date"     minOccurs="1" maxOccurs="1"/>
            <element name="referenceId"         type="xsd:int"      minOccurs="0" maxOccurs="1"/>
            <element name="amountIncludesTax"   type="xsd:boolean"  minOccurs="0" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="DonationPayment">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Donation payment details. Extends the generic payment structure
            </xsd:documentation>
        </xsd:annotation>
        <complexContent>
            <extension base="glis:PaymentType">
                <sequence>
                    <element name="amountCurrency" type="xsd:token" minOccurs="0" maxOccurs="1" />
                </sequence>
            </extension>
        </complexContent>
    </complexType>

    <complexType name="TaxType">
        <xsd:annotation>
            <xsd:documentation>
                Defines the generic Tax details
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="amount"              type="xsd:double"   minOccurs="1" maxOccurs="1"/>
            <element name="code"                type="xsd:token"    minOccurs="1" maxOccurs="1"/>
            <element name="rate"                type="xsd:double"   minOccurs="1" maxOccurs="1"/>
            <element name="status"              type="xsd:token"    minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="RegistrationFee">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Charity Registration fee object
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="charityReference"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="FeeDetails"                 type="glis:PaymentType"     minOccurs="1" maxOccurs="1"/>
            <element name="VatDetails"                 type="glis:TaxType"         minOccurs="1" maxOccurs="1"/>
            <element name="eventRef"                   type="xsd:int"              minOccurs="0" maxOccurs="1"/>
            <element name="bankAccountUniqueId"        type="xsd:token"            minOccurs="1" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="WaiveRegistrationFee">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Charity Registration fee object
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <element name="creditNoteNumber"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="charityReference"           type="xsd:token"            minOccurs="1" maxOccurs="1"/>
            <element name="FeeDetails"                 type="glis:PaymentType"     minOccurs="1" maxOccurs="1"/>
            <element name="VatDetails"                 type="glis:TaxType"         minOccurs="1" maxOccurs="1"/>
            <element name="eventRef"                   type="xsd:int"              minOccurs="0" maxOccurs="1"/>
            <element name="invoiceNumber"              type="xsd:token"            minOccurs="0" maxOccurs="1"/>
            <element name="bankAccountUniqueId"        type="xsd:token"            minOccurs="0" maxOccurs="1"/>
        </sequence>
    </complexType>

    <complexType name="Batch">
        <xsd:annotation>
            <xsd:documentation>
                Defines the Batch attributes
            </xsd:documentation>
        </xsd:annotation>
        <sequence>
            <xsd:element name="BatchNumber"             type="xsd:token"   minOccurs="1" maxOccurs="1"/>
            <xsd:element name="BatchDate"               type="xsd:date"    minOccurs="1" maxOccurs="1"/>
            <xsd:element name="BatchType"               type="xsd:token"   minOccurs="0" maxOccurs="1"/>
            <xsd:element name="ProcessBatchOnErrors"    type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="BatchChunkId"            minOccurs="1" maxOccurs="1">
                <xsd:simpleType>
                    <xsd:restriction base="xsd:token">
                        <xsd:minLength value="1"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </sequence>
    </complexType>

    <xsd:complexType name="Charity">
        <xsd:annotation>
            <xsd:documentation>
                Components of a Charity Organisation
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="charityStatus"          type="glis:CharityStatusType" minOccurs="1" maxOccurs="1"/>
            <xsd:element name="transactionDate"        type="xsd:date"         minOccurs="1" maxOccurs="1"/>
            <xsd:element name="charityName"            type="xsd:token"        minOccurs="1" maxOccurs="1"/>
            <xsd:element name="registrationNumber"     type="xsd:token"        minOccurs="1" maxOccurs="1"/>
            <xsd:element name="vmgCharityRefId"        type="xsd:token"        minOccurs="1" maxOccurs="1"/>
            <xsd:element name="category"               type="xsd:token"        minOccurs="0" maxOccurs="1"/>
            <xsd:element name="paymentGroup"           type="xsd:token"        minOccurs="0" maxOccurs="1"/>
            <xsd:element name="registeredAddress"      type="glis:Address"     minOccurs="1" maxOccurs="1"/>
            <xsd:element name="administrationAddress"  type="glis:Address"     minOccurs="0" maxOccurs="1"/>
            <xsd:element name="bankAccounts"           type="glis:BankAccountType" minOccurs="0" maxOccurs="unbounded"/>
            <xsd:element name="eventRef"               type="xsd:int"          minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="CharitiesBatch">
        <xsd:annotation>
            <xsd:documentation>
                Defines the batch request for charities
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Header"  type="header:MessageHeader"/>
            <xsd:element name="Batch"   type="glis:Batch"/>
            <xsd:element name="Charity" type="glis:Charity" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="RegistrationFeesBatch">
        <xsd:annotation>
            <xsd:documentation>
                Defines the batch request for Registration fee from charities
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Header"          type="header:MessageHeader"/>
            <xsd:element name="Batch"           type="glis:Batch"/>
            <xsd:element name="RegistrationFee" type="glis:RegistrationFee" minOccurs="1" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:complexType name="WaiveRegistrationFeesBatch">
        <xsd:annotation>
            <xsd:documentation>
                Defines the batch request for Registration fee waiver
            </xsd:documentation>
        </xsd:annotation>
        <xsd:sequence>
            <xsd:element name="Header"          type="header:MessageHeader"/>
            <xsd:element name="Batch"           type="glis:Batch"/>
            <xsd:element name="RegistrationFee" type="glis:WaiveRegistrationFee" minOccurs="1" maxOccurs="unbounded"/>
